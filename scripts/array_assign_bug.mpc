

array = MultiArray([5, 10], sfix)
array.assign(sfix(0))

vector = sfix(1. / 10.)

entry_sizes = Array(5, cint)
entry_sizes.assign_all(cint(1))

print_ln("Array before %s", array.reveal_nested())
@for_range_opt(cint(5))
def assign_array(idx):
    array.get_part(idx, entry_sizes[idx]).assign_all(vector)

print_ln("Array after %s", array.reveal_nested())